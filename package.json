    "font-awesome": "^4.6.3",
    "postcss": "^5.1.2",
    "react": "^15.3.1",
    "react-dom": "^15.3.1",
    "react-redux": "^4.4.5",
    "react-router": "^2.8.0",
    "react-router-redux": "^4.0.5",
    "react-select": "^1.0.0-rc.1",
